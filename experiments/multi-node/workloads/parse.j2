"width","seconds","total_iops","avg_iops","avg_lat"
{% for r in results.results %}
{% for line in r.stdout_lines %}
{% if 'period' in line %}
{% set x = line.split(' ') %}
{% set sec = x[1] %}
{% set total_iops = x[3] %}
{% set avg_iops = x[5] %}
{% set avg_lat = x[7] %}
"{{ r.item }}","{{ sec }}","{{ total_iops }}","{{ avg_iops }}","{{ avg_lat }}"
{% endif %}
{% endfor %}
{% endfor %}
